{
  "version": 2,
  "builds": [
    {
      "src": "dest/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ],
  "functions": {
    "dist/*.js": {
      "maxDuration": 60
    }
  }
}
